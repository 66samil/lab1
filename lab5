def musbet_elementlerin_cemi(massiv):
    cəm = 0
    for element in massiv:
        if element > 0:  # Əgər element müsbətdirsə
            cəm += element  # Müsbət elementi cəmdə toplayırıq
    return cəm
massiv = [3, -1, 4, 0, -2, 7, 5]
nəticə = musbet_elementlerin_cemi(massiv)
print("Müsbət elementlərin cəmi:", nəticə)
